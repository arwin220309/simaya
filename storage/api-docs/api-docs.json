{
    "swagger": "2.0",
    "info": {
        "title": "@ragasubekti/siMAYA",
        "description": "Dokumentasi API siMAYA",
        "contact": {
            "email": "ragasubekti@outlook.com"
        },
        "version": "1.0.0"
    },
    "host": "simaya.id",
    "basePath": "/api",
    "schemes": [
        "http"
    ],
    "paths": {
        "/authentication": {
            "post": {
                "tags": [
                    "Login"
                ],
                "summary": "Autentikasi User dan Token",
                "operationId": "userAuthentication",
                "parameters": [
                    {
                        "name": "body",
                        "in": "body",
                        "description": "Isi Request",
                        "required": true,
                        "schema": {
                            "required": [
                                "nip",
                                "password"
                            ],
                            "properties": {
                                "username": {
                                    "type": "string"
                                },
                                "password": {
                                    "type": "string",
                                    "default": "123456"
                                }
                            },
                            "type": "object"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                }
            }
        }
    },
    "definitions": {},
    "securityDefinitions": {
        "JWT": {
            "type": "apiKey",
            "description": "Untuk mengakses beberapa API dibutuhkan JWT, contoh header: Bearer xxxx.xxxxxxxxxx.xxxx",
            "name": "Authorization",
            "in": "header",
            "scopes": {}
        }
    }
}